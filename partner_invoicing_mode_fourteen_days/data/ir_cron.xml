<odoo noupdate="1">
    <record id="ir_cron_generate_fourteen_days_invoice" model="ir.cron">
        <field name="name">Generate 14 days Invoices</field>
        <field eval="False" name="active" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
        <field name="model_id" ref="model_sale_order" />
        <field name="code">model.cron_generate_fourteen_days_invoices()</field>
        <field
            name="nextcall"
            eval="(DateTime.now().replace(hour=1,minute=0).strftime('%Y-%m-%d %H:%M:%S'))"
        />
    </record>
</odoo>
