<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Sequence -->
    <record id="seq_billing_option_1" model="ir.sequence">
        <field name="name">Billing: Option 1</field>
        <field name="padding" eval="5" />
        <field name="prefix">B/</field>
    </record>
    <!-- Sequence Option Config -->
    <record id="billing_sequence_option" model="ir.sequence.option">
        <field name="name">Billing</field>
        <field name="model">account.billing</field>
        <field name="use_sequence_option" eval="False" />
    </record>
    <record id="billing_sequence_option_line_1" model="ir.sequence.option.line">
        <field
            name="base_id"
            ref="account_billing_sequence_option.billing_sequence_option"
        />
        <field name="name">Billing Option 1</field>
        <field name="filter_domain">[]</field>
        <field
            name="sequence_id"
            ref="account_billing_sequence_option.seq_billing_option_1"
        />
    </record>
</odoo>
